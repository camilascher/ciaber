	<queryString language="SQL">
		<![CDATA[SELECT
                prod.idproducto as Codigo,
                prod.descripcion as Producto,
                sum(det.cantidad) as Cantidad,
                sum(det.cantidad*det.precio) as Total,
                usr.nombre as Turno
                FROM
			ABMPrueba.pedido ped,
                ABMPrueba.detalle_pedido det,
                ABMPrueba.producto prod,
                ABMPrueba.usuario usr
                WHERE
                prod.idproducto = det.idproducto
			and det.idpedido = ped.idpedido
                and usr.idusuario = ped.usuarioid_creacion
                and str_to_date(ped.fecha,'%Y-%m-%d') between str_to_date('01/01/2017','%d/%m/%Y') and str_to_date('01/10/2017','%d/%m/%Y') 
                and usr.nombre = 'Cami'
                group by Codigo,Producto;]]>
	</queryString>
